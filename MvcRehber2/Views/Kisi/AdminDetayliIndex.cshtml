<body>
	@*<button class="btn btn-info" onclick="location.href='@Url.Action("Ekle", "Kisi")'">Ekle</button>&nbsp;&nbsp;*@

	<br />
	<h2>Üyeler ve Kişileri</h2>

	<div class="container-table300">
		<div class="wrap-table100">

			<div class="table100 ver1 m-t-50 m-b-110">
				<div class="table100-head">
					<table>
						<thead>
							<tr class="row100 head">
								<th class="cell100 column1">Uye ID</th>
								<th class="cell100 column2">Kisi Adı</th>
								<th class="cell100 column3">Kisi Soyadı</th>
								<th class="cell100 column4">Kisi Maili</th>
								<th class="cell100 column4">Kisi Telefonu</th>
							</tr>
						</thead>
					</table>
				</div>
				<div class="table100-body js-pscroll">
					<table>
						<tbody>



							@foreach (var kisi in Model.Kisi)
							{
								
									<tr class="row100 body">
										<td class="cell100 column1">@kisi.UyeId</td>
										<td class="cell100 column3">@kisi.Ad</td>
										<td class="cell100 column4">@kisi.Soyad</td>
										<td class="cell100 column4">@kisi.EmailAdres</td>
										<td class="cell100 column4">@kisi.CepTelefon</td>

									</tr>
								






							}
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>


	<script src="vendor/jquery/jquery-3.2.1.min.js"></script>

	<script src="vendor/bootstrap/js/popper.js"></script>
	<script src="vendor/bootstrap/js/bootstrap.min.js"></script>

	<script src="vendor/select2/select2.min.js"></script>

	<script src="vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
	<script>
		$('.js-pscroll').each(function() {
			var ps = new PerfectScrollbar(this);

			$(window).on('resize', function() {
				ps.update();
			})
		});


	</script>

